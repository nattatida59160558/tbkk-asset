@page
@model tbkk_AC.Pages.Positions.ImportModel
@{
    ViewData["Title"] = "Import";
}

<h2>Import</h2>
<form method="post" enctype="multipart/form-data">
    <div class="row">
        <div class="col-md-4">
            <input type="file" id="fUpload" name="files" class="form-control" />
        </div>
        <div class="col-md-8">
            <input type="button" id="btnUpload" value="Upload" />
        </div>
    </div>
    <div class="row">
        <div class="col-md-8" style="padding-top:10px;">
            <button asp-page-handler="Export">Export</button>
        </div>
    </div>
    <br />
    <div id="dvData"></div>
</form>


<script src="../js/site.min.js"></script>
<environment include="Development">
    <script src="~/js/site.js" asp-append-version="true" defer></script>
</environment>
<environment exclude="Development">
    <script src="~/js/site.min.js" asp-append-version="true" defer></script>
</environment>